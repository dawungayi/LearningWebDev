<!-- Index page for the RESTful blog app -->

<%- include("partials/header.ejs") %>

<h1>THIS IS THE INDEX PAGE</h1>
<% blogs.forEach(blog => { %>
<div>
    <h3>  <%= blog.title  %> </h3>
    <div> Date Created: <%= blog.created %> </div>
    <div> <img src=" <%= blog.image %> " alt="" style="height: 200px;"> </div>
    <p>
        <%= blog.body %> 
    </p>
    <% }) %>
</div>

<%- include("partials/footer.ejs") %>
